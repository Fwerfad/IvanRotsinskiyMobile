<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Native mobile testing Suite">
    <parameter name="platformName" value="iOS"/>
    <parameter name="appType" value="native"/>
    <parameter name="deviceName" value="IPAD 11,1"/>
    <parameter name="platformVersion" value="14.1"/>
    <parameter name="bundleid" value="com.epam.EPAMTestApp.ii"></parameter>

    <test name="Native tests">
        <groups>
            <run>
                <include name="native"/>
            </run>
        </groups>
        <classes>
            <class name="scenarios.nativeMobileTests"/>
        </classes>
    </test>
</suite>